{{ form_start(form) }}
    {{ form_row(form.image) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.website) }}
    {{ form_row(form.phone) }}
    {{ form_row(form.delivery) }}
    {{ form_row(form.take_away) }}
    {{ form_row(form.avg_price) }}
    {{ form_row(form.address) }}
    <div class="mb-3" {{ stimulus_controller('google-place', {'page' : 'form', 'form': 'shop' }) }}>
            <label class="form-label" for="searchBarAutocomplete">Adresse</label>
            <input type="text"
            id="searchBarAutocomplete"
            aria-label="Adresse" aria-describedby="btn-search-address" class="form-control"
            {% if shop is defined %}
                value="{{ shop.address }}"
            {% endif %}
            > 
            
    </div>
    {{ form_row(form.category) }}
    <button type="submit" class="btn btn-primary">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
